<head>
  <link href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet">
  <link href="/weather/view/weatherCss.css" rel="stylesheet">
  <link href="/weather/view/css/weather-icons.css" rel="stylesheet">
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.5/angular.min.js"></script>
  <script src="http://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.10/angular-ui-router.js"></script>
  <script src="/weather/controller/controller.js"></script>
  <script src="http://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.10/angular-ui-router.js"></script>
  
</head>
<body ng-app="Weather" ng-controller="WeatherCtrl">
	<div class="overlay">
	</div>
	<div class="container-fluid weather-box text-center">
			<div class="row text-center">
				<div class="col-xs-12 cityName">
					{{city}}
				</div>
			</div>
			<div class="row">
				<div class="col-sm-6 condition">
					{{condition}} <br><i class="wi {{icon}}"></i>
				</div>	
				<div class="col-sm-6 temp">
					<p>Temp</p>{{temperature}}
				</div>
			</div>
		</div>
</body>